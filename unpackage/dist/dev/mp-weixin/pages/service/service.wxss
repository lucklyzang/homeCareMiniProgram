page {
  width: 100%;
  height: 100%;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
}
.content-box .top-area-box {
  position: relative;
  width: 100%;
  height: 230px;
}
.content-box .top-area-box  .nav {
  width: 100%;
  background: #fff;
  position: absolute;
  top: 0;
  left: 0;
}
.content-box .top-area-box  .nav .header_fixed .header_title_center {
  color: #fff !important;
}
.content-box .top-area-box  .nav .header_fixed .header_title_center text {
  color: #fff !important;
}
.content-box .top-area-box .search-box {
  position: absolute;
  top: 100px;
  left: 3%;
  width: 94%;
  z-index: 10;
}
.content-box .top-area-box > image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 230px;
}
.content-box .content-area-box {
  flex: 1;
  z-index: 10;
  height: 0;
  margin-top: -80px;
  display: flex;
}
.content-box .content-area-box .content-left {
  width: 25%;
  height: 100%;
  overflow: auto;
  background: #F5F5F5;
}
.content-box .content-area-box .content-left .service-list {
  height: 100%;
  height: 48px;
  width: 100%;
}
.content-box .content-area-box .content-left .service-list > text {
  width: 100%;
  display: inline-block;
  height: 48px;
  text-align: center;
  line-height: 48px;
  font-size: 13px;
  color: #666667;
}
.content-box .content-area-box .content-left .service-list .selectServieTypeStyle {
  font-size: 14px !important;
  color: #F16C8C !important;
  background: #fff !important;
}
.content-box .content-area-box .content-right {
  width: 75%;
  height: 100%;
  overflow: auto;
  padding: 10px 0 10px 10px;
  box-sizing: border-box;
  background: #fff;
}
.content-box .content-area-box .content-right .content-list {
  display: flex;
  padding: 14px 0;
  box-sizing: border-box;
  align-items: center;
  justify-content: space-between;
  position: relative;
  border: none;
}
.content-box .content-area-box .content-right .content-list:after {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  background-color: rgba(157, 156, 156, 0.8);
  -webkit-transform: scaleY(0.5);
          transform: scaleY(0.5);
}
.content-box .content-area-box .content-right .content-list .content-list-right {
  margin-left: 10px;
  padding-right: 10px;
  box-sizing: border-box;
  flex: 1;
  width: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.content-box .content-area-box .content-right .content-list .content-list-right .service-introduce {
  flex: 1;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.content-box .content-area-box .content-right .content-list .content-list-right .service-introduce .service-name > text {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  width: 100%;
  display: inline-block;
  font-size: 14px;
  margin-bottom: 20px;
  color: #101010;
  font-weight: bold;
}
.content-box .content-area-box .content-right .content-list .content-list-right .service-introduce .service-price > text {
  font-size: 14px;
  color: #E86F50;
}

