page {
  width: 100%;
  height: 100%;
}
.content-box {
  display: flex;
  min-height: 100vh;
  height: 100%;
  width: 100%;
  flex-direction: column;
  padding-bottom: 0;
  background: #f5f5f5;
}
.content-box ::-webkit-scrollbar {
  width: 0;
  height: 0;
  background-color: transparent;
}
.content-box .uni-ec-canvas {
  width: 100%;
  height: 100px;
  display: block;
}
.content-box .top-area {
  height: 50px;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10px;
  box-sizing: border-box;
  width: 100%;
  margin: 44px 0 10px 0;
}
.content-box .top-area .top-area-left {
  flex: 1;
}
.content-box .top-area .top-area-left  .u-search .u-content {
  height: 35px !important;
}
.content-box .top-area .top-area-right {
  width: 22%;
  text-align: right;
}
.content-box .top-area .top-area-right > image {
  width: 24px;
  height: 24px;
  vertical-align: middle;
}
.content-box .top-area .top-area-right > image:nth-child(1) {
  margin-right: 14px;
}
.content-box .banner-area-box {
  padding: 0 10px 10px 10px;
  min-height: 135px;
  box-sizing: border-box;
}
.content-box .center-area {
  flex: 1;
  overflow: auto;
  position: relative;
}
.content-box .center-area  .u-empty {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.content-box .center-area .nurse-type {
  height: 90px;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.content-box .center-area .nurse-type .nurse-type-list {
  flex: 1;
  height: 73px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.content-box .center-area .nurse-type .nurse-type-list > image {
  width: 46px;
  height: 46px;
}
.content-box .center-area .nurse-type .nurse-type-list > text {
  font-size: 12px;
  color: #898C8C;
}
.content-box .center-area .latest-news {
  display: flex;
  margin-top: 10px;
  align-items: center;
  justify-content: space-between;
  height: 63px;
  width: 100%;
  padding: 0 0 0 20px;
  background: #fff;
  box-sizing: border-box;
}
.content-box .center-area .latest-news .latest-news-left {
  margin-right: 20px;
}
.content-box .center-area .latest-news .latest-news-left > image {
  width: 36px;
  height: 52px;
  vertical-align: middle;
}
.content-box .center-area .latest-news .latest-news-right {
  flex: 1;
  padding-right: 10px;
  box-sizing: border-box;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.content-box .center-area .latest-news .latest-news-right .new-content-list {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  width: 100%;
  font-size: 12px;
  color: #2C2C2C;
}
.content-box .center-area .latest-news .latest-news-right .new-content-list:last-child {
  margin-top: 10px;
}
.content-box .center-area .recommend-service {
  width: 98%;
  margin: 0 auto;
  margin-top: 10px;
  border-radius: 7px;
  padding: 10px;
  box-sizing: border-box;
  background: #fff;
  box-shadow: 0px 1px 4px 0 rgba(0, 0, 0, 0.12);
}
.content-box .center-area .recommend-service .service-title {
  display: flex;
  align-items: center;
}
.content-box .center-area .recommend-service .service-title .service-title-left {
  font-size: 16px;
  color: #101010;
  font-weight: bold;
  margin-right: 10px;
}
.content-box .center-area .recommend-service .service-title .service-title-right {
  font-size: 12px;
  color: #888888;
  margin-top: 4px;
}
.content-box .center-area .recommend-service .service-list {
  margin-top: 10px;
  padding: 4px 8px;
  height: 75px;
  box-sizing: border-box;
  background: #F8F8F8;
