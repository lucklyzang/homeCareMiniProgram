  flex: 1;
  width: 0;
}
.content-box .center-area .recommend-service .service-list .service-list-right .service-project > text {
  display: inline-block;
  width: 100%;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.content-box .center-area .recommend-service .service-list .service-list-right .service-project > text:first-child {
  font-size: 16px;
  color: #101010;
  font-weight: bold;
  margin-bottom: 12px;
}
.content-box .center-area .recommend-service .service-list .service-list-right .service-project > text:last-child {
  font-size: 12px;
  color: #888888;
}
.content-box .center-area .recommend-service .service-list .service-list-right .service-price > text {
  font-size: 16px;
  color: #E95E5E;
}
